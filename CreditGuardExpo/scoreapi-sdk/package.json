  "author": "StitchCredit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stitchcredit/scoreapi-sdk"
  },
  "bugs": {
    "url": "https://github.com/stitchcredit/scoreapi-sdk/issues"
  },
